#! /bin/sh

SED="/usr/bin/sed"
prefix="/home/runner/work/php-build-scripts/php-build-scripts/bin/php7"
datarootdir="/home/runner/work/php-build-scripts/php-build-scripts/bin/php7/php"
exec_prefix="/home/runner/work/php-build-scripts/php-build-scripts/bin/php7"
version="8.4.13"
vernum="80413"
include_dir="${prefix}/include/php"
lib_dir="${exec_prefix}/lib"
includes="-I$include_dir -I$include_dir/main -I$include_dir/TSRM -I$include_dir/Zend -I$include_dir/ext -I$include_dir/ext/date/lib"
ldflags=" -L/home/runner/work/php-build-scripts/php-build-scripts/bin/php7/lib -L/home/runner/work/php-build-scripts/php-build-scripts/bin/php7"
libs="  -lyaml -lsnappy -lrdkafka -lleveldb -lgrpc -lgpr -lgpr -lrt -lrt -ldl -ldl -lpthread -lpthread -lstdc++ -lm  -lpthread -lxml2 -lssl -lcrypto -lsqlite3 -lz -lssl -lcrypto -lcurl -lxml2 -lffi -lssl -lcrypto -lz -lpng16 -ljpeg -lgmp -lstdc++ -ldeflate -lsqlite3 -lxml2 -lxml2 -lxml2 -lxml2 -lzip -lzstd -lz -lssl -lcrypto"
extension_dir="/home/runner/work/php-build-scripts/php-build-scripts/bin/php7/lib/php/extensions/no-debug-zts-20240924"
man_dir=`eval echo ${datarootdir}/man`
program_prefix=""
program_suffix=""
exe_extension=""
php_cli_binary=NONE
php_cgi_binary=NONE
configure_options=" '--prefix=/home/runner/work/php-build-scripts/php-build-scripts/bin/php7' '--exec-prefix=/home/runner/work/php-build-scripts/php-build-scripts/bin/php7' '--with-curl' '--with-zlib' '--with-gmp' '--with-yaml' '--with-openssl' '--with-zip' '--with-libdeflate' '--with-jpeg' '--enable-gd' '--with-ffi' '--with-rdkafka=/home/runner/work/php-build-scripts/php-build-scripts/bin/php7' '--with-leveldb=/home/runner/work/php-build-scripts/php-build-scripts/bin/php7' '--with-snappy-includedir=/home/runner/work/php-build-scripts/php-build-scripts/bin/php7' '--without-readline' '--disable-debug' '--enable-chunkutils2' '--enable-morton' '--enable-mbstring' '--disable-mbregex' '--enable-calendar' '--enable-pmmpthread' '--enable-fileinfo' '--with-libxml' '--enable-xml' '--enable-dom' '--enable-simplexml' '--enable-xmlreader' '--enable-xmlwriter' '--disable-cgi' '--disable-phpdbg' '--disable-session' '--without-pear' '--without-iconv' '--with-pdo-sqlite' '--with-pdo-mysql' '--with-pic' '--with-libzstd' '--enable-phar' '--enable-ctype' '--enable-sockets' '--enable-shared=no' '--enable-static=yes' '--enable-shmop' '--enable-zts' '--disable-short-tags' '--enable-pcntl' '--enable-mysqlnd' '--with-mysqli=mysqlnd' '--enable-bcmath' '--enable-cli' '--enable-ftp' '--enable-grpc=/home/runner/work/php-build-scripts/php-build-scripts/bin/php7' '--enable-protobuf' '--enable-zstd' '--enable-snappy' '--enable-vanillagenerator' '--enable-opcache=yes' '--enable-opcache-jit=yes' '--enable-igbinary' '--with-crypto' '--enable-recursionguard' '--enable-xxhash' '--enable-arraydebug' '--enable-encoding' '--without-valgrind' 'PKG_CONFIG_PATH=/home/runner/work/php-build-scripts/php-build-scripts/bin/php7/lib/pkgconfig' 'CFLAGS=-O2 -fPIC -march=x86-64 -mtune=skylake -g -m64 -O2 -march=x86-64 -mtune=skylake -g -m64 -O2 -ftree-vectorize -fomit-frame-pointer -march=x86-64 -mtune=skylake -g -m64 -O2 -march=x86-64 -mtune=skylake -g -m64 -O2 -ftree-vectorize -fomit-frame-pointer -funsafe-loop-optimizations -fpredictive-commoning -ftracer -ftree-loop-im -frename-registers -fcx-limited-range -funswitch-loops -fivopts -fno-gcse ' 'LDFLAGS=-Wl,-rpath='"
php_sapis=" cli"
ini_dir=""
ini_path="/home/runner/work/php-build-scripts/php-build-scripts/bin/php7/lib"
php_embed_type=""

# Set php_cli_binary and php_cgi_binary if available
for sapi in $php_sapis; do
  case $sapi in
  cli)
    php_cli_binary="${exec_prefix}/bin/${program_prefix}php${program_suffix}${exe_extension}"
    ;;
  cgi)
    php_cgi_binary="${exec_prefix}/bin/${program_prefix}php-cgi${program_suffix}${exe_extension}"
    ;;
  esac
done

# Determine which (if any) php binary is available
if test "$php_cli_binary" != "NONE"; then
  php_binary="$php_cli_binary"
else
  php_binary="$php_cgi_binary"
fi

# Set embed SAPI library path
if test "$php_embed_type" = "shared"; then
  php_embed_lib=libphp.so
elif test "$php_embed_type" = "static"; then
  php_embed_lib=libphp.a
fi

# Remove quotes
configure_options=`echo $configure_options | $SED -e "s#'##g"`

case "$1" in
--prefix)
  echo $prefix;;
--includes)
  echo $includes;;
--ldflags)
  echo $ldflags;;
--libs)
  echo $libs;;
--extension-dir)
  echo $extension_dir;;
--include-dir)
  echo $include_dir;;
--lib-dir)
  echo $lib_dir;;
--lib-embed)
  echo $php_embed_lib;;
--php-binary)
  echo $php_binary;;
--php-sapis)
  echo $php_sapis;;
--configure-options)
  echo $configure_options;;
--man-dir)
  echo $man_dir;;
--ini-path)
  echo $ini_path;;
--ini-dir)
  echo $ini_dir;;
--version)
  echo $version;;
--vernum)
  echo $vernum;;
*)
  cat << EOF
Usage: $0 [OPTION]
Options:
  --prefix            [$prefix]
  --includes          [$includes]
  --ldflags           [$ldflags]
  --libs              [$libs]
  --extension-dir     [$extension_dir]
  --include-dir       [$include_dir]
  --lib-dir           [$lib_dir]
  --lib-embed         [$php_embed_lib]
  --man-dir           [$man_dir]
  --php-binary        [$php_binary]
  --php-sapis         [$php_sapis]
  --ini-path          [$ini_path]
  --ini-dir           [$ini_dir]
  --configure-options [$configure_options]
  --version           [$version]
  --vernum            [$vernum]
EOF
  exit 1;;
esac

exit 0
